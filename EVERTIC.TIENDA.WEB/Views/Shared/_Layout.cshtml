    <script type="text/javascript">

        var contador = 0;

        var rootHost = "@Scripts.Url("~/")";

        $(function () {



        });
        function onAjaxBeginForm() {

            $("body").css("cursor", "progress");
            waitingDialog.show();
        }

        function onAjaxCompleteForm() {

            $("body").css("cursor", "default");
            waitingDialog.hide();
        }

        $(document).ajaxStart(function () {
            onAjaxBeginForm();
        });

        $(document).ajaxStop(function () {
            onAjaxCompleteForm();
        });

        $(window).on('beforeunload', function () {
            $('*').css("cursor", "progress");
            waitingDialog.show();
        });

    </script>
